{
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "forceAllTransforms": true,
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "useBuiltIns": true
      }
    ]
  ],
  "plugins" : [
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-proposal-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}